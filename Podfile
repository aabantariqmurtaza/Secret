# platform :ios, '9.0'

target 'whatsApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for whatsApp

  pod 'Firebase/Core'
  pod 'Firebase/Messaging'
  pod 'GoogleSignIn'
  pod 'Firebase/Auth'
  pod 'IQKeyboardManagerSwift'
  pod 'Firebase/Database'

end
